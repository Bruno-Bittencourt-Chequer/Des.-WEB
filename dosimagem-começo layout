OBS:Componente Retangulo

import React from 'react';


function Clinica() {
  return (
    <div className="clinica">
      <div className="header">
        <h2>Clínica XYZ</h2>
      </div>
      <div className="content">
        <div className="section">
          <h3>Informações Gerais</h3>
          <p>Texto com informações gerais sobre a clínica.</p>
        </div>
        <div className="section">
          <h3>Horário de Funcionamento</h3>
          <p>Horário de funcionamento da clínica.</p>
        </div>
        <div className="section">
          <h3>Serviços Oferecidos</h3>
          <ul>
            <li>Serviço 1</li>
            <li>Serviço 2</li>
            <li>Serviço 3</li>
          </ul>
        </div>
      </div>
      <div className="footer">
        <button className="download-button">Download</button>
      </div>
    </div>
  );
}

export default Clinica;



OBS:componete clinica.css

  .clinica {
  width: 400px;
  background-color: #fff;
  border: 2px solid #000;
  border-radius: 5px;
  padding: 20px;
}

.header {
  text-align: center;
}

.content {
  margin-top: 20px;
}

.section {
  margin-bottom: 20px;
}

.footer {
  text-align: center;
}

.download-button {
  padding: 10px 20px;
  background-color: #007bff;
  color: #fff;
  border: none;
  border-radius: 5px;
  cursor: pointer;
  font-size: 16px; 
  transition: background-color 0.3s ease; 
  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.2);
}

.download-button:hover {
  background-color: #0056b3;
}




OBS: index.js


 import React from "react";
import ReactDOM from "react-dom";
import { BrowserRouter, Route, Routes } from "react-router-dom";
import { createRoot } from "react-dom/client";

import Home from "./pages/Home";
import Fotos from "./pages/Fotos";
import Login from "./pages/Login";
import Logout from "./pages/Logout";
import { auth } from "./config/Firebase";
import { onAuthStateChanged } from "firebase/auth";
import Atletas from "./pages/Atletas";
import Freddie from "./pages/FreddieMercury";
import Clinica from "./pages/Clinica";
import Modelagem from "./pages/Modelagem";
import Radiosinoviortese from "./pages/Radiosinoviortese";
import SegmentacaoeQuantificacao from "./pages/SegmentacaoeQuantificacao";
import DosimetriaPreClinica from "./pages/DosimetriaPreClinica";

const container = document.getElementById("root");
const root = createRoot(container);

// Removed rendering of Clinica from ReactDOM.render()

onAuthStateChanged(auth, (user) => {
  if (user) {
    window.sessionStorage.setItem("accessToken", user.accessToken);
  } else {
    window.sessionStorage.removeItem("accessToken");
  }
});

root.render(
  <React.StrictMode>
    <BrowserRouter>
      <Routes>
        <Route path="/" element={<Home />} />
        <Route path="/freddie" element={<Freddie />} />
        <Route path="/fotos" element={<Fotos />} />
        <Route path="/atletas" element={<Atletas />} />
        <Route path="/login" element={<Login />} />
        <Route path="/logout" element={<Logout />} />
        {/* Removed rendering of Clinica here */}
        <Route path="/clinica" element={<Clinica />} />
        <Route path="/modelagem" element={<Modelagem />} />
        <Route path="/radiosinoviortese" element={<Radiosinoviortese />} />
        <Route
          path="/SegmentacaoeQuantificacao"
          element={<SegmentacaoeQuantificacao />}
        />
        <Route
          path="/dosimetriapreclinica"
          element={<DosimetriaPreClinica />}
        />
      </Routes>
    </BrowserRouter>
  </React.StrictMode>,
  container
);
